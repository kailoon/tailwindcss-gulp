{
  "name": "tailwindcss-gulp",
  "version": "1.0.0",
  "description": "a quick html starter kit using Tailwindcss.",
  "keywords": [
    "tailwindcss",
    "gulp"
  ],
  "author": "kailoon",
  "license": "MIT",
  "repository": {
    "type": "ISC",
    "url": ""
  },
  "scripts": {
    "dev": "gulp",
    "build": "gulp build"
  },
  "options": {
    "config": {
      "tailwindjs": "./tailwind.config.js"
    },
    "paths": {
      "root": "./",
      "src": {
        "base": "./src",
        "css": "./src/scss",
        "js": "./src/js",
        "img": "./src/img"
      },
      "dist": {
        "base": "./dist",
        "css": "./dist/css",
        "js": "./dist/js",
        "img": "./dist/img"
      },
      "build": {
        "base": "./_site",
        "css": "./_site/css",
        "js": "./_site/js",
        "img": "./_site/img"
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "browser-sync": "^2.26.7",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-purgecss": "^2.2.0",
    "gulp-sass": "^4.1.0",
    "log-symbols": "^4.0.0",
    "tailwindcss": "^1.4.6"
  }
}
